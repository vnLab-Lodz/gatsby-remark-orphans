{
  "name": "gatsby-remark-orphans",
  "repository": {
    "type": "git",
    "url": "https://github.com/vnLab-Lodz/gatsby-remark-orphans.git"
  },
  "version": "1.0.5",
  "description": "Gatsby plugin for removing orphans in markdown text.",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "lint": "eslint ./src/** --ext .ts,.tsx",
    "test": "jest",
    "link": "tsc && copy /Y package.json dist && copy /Y readme.md dist && cd dist && npm link",
    "build": "tsc && copy /Y package.json dist && copy /Y readme.md dist",
    "publish-npm": "tsc && copy /Y package.json dist && copy /Y readme.md dist && npm publish"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-remark-plugin",
    "markdown",
    "widows",
    "orphans"
  ],
  "author": "Piotr Karczewski",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^27.4.0",
    "@types/mdast": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-jest": "^27.4.6",
    "eslint": "^8.8.0",
    "jest": "^27.4.7",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "unist-util-visit": "^2.0.3"
  }
}
